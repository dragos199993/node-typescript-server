{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "jsx-no-lambda": false,
        "class-name": true,
        "curly": true,
        "eofline": true,
        "object-literal-sort-keys": false,
        "quotemark": [
          true,
          "single",
          "jsx-double"
        ],
        "indent": [
          true,
          "spaces",
          2
        ],
        "jsx-curly-spacing": [
          true,
          "always"
        ],
        "max-line-length": [
          true,
          130
        ],
        "ordered-imports": [
          false
        ],
        "no-arg": true,
        "no-consecutive-blank-lines": true,
        "no-console": [
          false
        ],
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-shadowed-variable": true,
        "trailing-comma": false,
        "no-trailing-whitespace": true,
        "no-unused-expression": true,
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "no-var-requires": false,
        "prefer-for-of": false,
        "one-line": [
          true,
          "check-catch",
          "check-finally",
          "check-else",
          "check-open-brace",
          "check-whitespace"
        ],
        "member-access": false,
        "radix": true,
        "semicolon": [
          true,
          "always",
          "ignore-bound-class-methods"
        ],
        "switch-default": true,
        "typedef": [
          true,
          "call-signature",
          "parameter",
          "property-declaration",
          "variable-declaration",
          "member-variable-declaration"
        ]
      },
    
    "rulesDirectory": []
}